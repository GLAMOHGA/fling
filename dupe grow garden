local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "dup Grow a garden",
    LoadingTitle = "dup Grow a garden",
    LoadingSubtitle = "by https://t.me/vomagla",
    ConfigurationSaving = {
        Enabled = false
    }
})

local MainTab = Window:CreateTab("Дюп", 4483362458)

-- https://t.me/vomagla
MainTab:CreateButton({
    Name = "2x Инвентарь",
    Callback = function()
        local player = game.Players.LocalPlayer
        if not player then
            warn("Player not found!")
            return
        end

        local backpack = player:FindFirstChild("Backpack")
        local character = player.Character or player.CharacterAdded:Wait()

        local function doubleTools(container)
            local tools = {}
            for _, item in pairs(container:GetChildren()) do
                if item:IsA("Tool") and item.Name ~= "Shovel [Destroy Plants]" then
                    table.insert(tools, item)
                end
            end
            for _, tool in pairs(tools) do
                local success, clone = pcall(function()
                    return tool:Clone()
                end)
                if success and clone then
                    clone.Parent = container
                end
            end
        end

        if backpack then doubleTools(backpack) end
        if character then doubleTools(character) end
    end,
})

-- Кнопка для копирования ссылки на телеграм канал
MainTab:CreateButton({
    Name = "Нажми, чтоб скопировать сыллка на лутчший канал по скриптам роблокс",
    Callback = function()
        local text = "https://t.me/vomagla"
        setclipboard(text)
    end,
})

-- Параграф с описанием
MainTab:CreateParagraph({
    Title = "dup Grow a garden",
    Content = "самый лучший канал по скриптам роблокс: https://t.me/vomagla"
})
